{
  "name": "parcel-starter",
  "version": "2.0.0",
  "description": "A nice little starter with Parcel",
  "src_template": "src/*.pug",
  "scripts": {
    "start": "parcel serve $npm_package_src_template",
    "build": "parcel build $npm_pacage_src_template --no-source-maps",
    "dev": "npm run start",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "parcelCleanPaths": [
    "dist/*.*",
    "build/*.*"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/JREAM/parcel-starter.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/JREAM/parcel-starter/issues"
  },
  "homepage": "https://github.com/JREAM/parcel-starter#readme",
  "dependencies": {
    "bulma": "^0.9.2",
    "svelte": "^3.34.0"
  },
  "devDependencies": {
    "@babel/core": "^7.13.8",
    "@babel/eslint-parser": "^7.13.8",
    "@babel/preset-env": "^7.13.8",
    "eslint": "^7.21.0",
    "eslint-config-eslint": "^7.0.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsdoc": "^32.2.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-svelte3": "^3.1.2",
    "parcel-bundler": "^1.12.4",
    "parcel-plugin-clean-easy": "^1.0.2",
    "pug": "^3.0.2",
    "sass": "^1.32.8"
  }
}
